generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String @unique
  password  String
  jwt_token String? @unique @default("")

  updated_at DateTime @updatedAt
  created_at DateTime @default(now())

  @@map("users")
}
